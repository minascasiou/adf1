{
	"name": "pipeline1",
	"properties": {
		"description": "import csv 1k into Cosmos SQL as JSON document\nHaNeCan Finance SQL Container",
		"activities": [
			{
				"name": "Copy data to Cosmos 2",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings",
							"skipLineCount": 0
						}
					},
					"sink": {
						"type": "CosmosDbSqlApiSink",
						"writeBehavior": "insert"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"sink": {
									"path": "$['id']"
								}
							},
							{
								"sink": {
									"path": "$['txnid']"
								}
							},
							{
								"sink": {
									"path": "$['customerid']"
								}
							},
							{
								"sink": {
									"path": "$['accountid']"
								}
							},
							{
								"sink": {
									"path": "$['categoryl1']"
								}
							},
							{
								"sink": {
									"path": "$['categoryl2']"
								}
							},
							{
								"sink": {
									"path": "$['categoryl3']"
								}
							},
							{
								"sink": {
									"path": "$['creationdate']"
								}
							},
							{
								"sink": {
									"path": "$['creationdateyyyymm']"
								}
							},
							{
								"sink": {
									"path": "$['creationdatetime']"
								}
							},
							{
								"sink": {
									"path": "$['txnamountcurrency']"
								}
							},
							{
								"sink": {
									"path": "$['runningbalancecurrency']"
								}
							},
							{
								"sink": {
									"path": "$['runningbalance']"
								}
							},
							{
								"sink": {
									"path": "$['txnamount']"
								}
							},
							{
								"sink": {
									"path": "$['merchantid']"
								}
							},
							{
								"sink": {
									"path": "$['txndescription']"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "DelimitedText2",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "CosmosDbSqlApiCollection2",
						"type": "DatasetReference"
					}
				]
			}
		],
		"concurrency": 1,
		"annotations": []
	}
}