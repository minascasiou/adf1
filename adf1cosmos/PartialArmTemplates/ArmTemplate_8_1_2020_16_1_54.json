											"name": "creationdateyyyymm",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "creationdateyyyymm",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "creationdatetime",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "creationdatetime",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "txnamountcurrency",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "txnamountcurrency",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "txnamount",
											"type": "Decimal",
											"physicalType": "String"
										},
										"sink": {
											"name": "txnamount",
											"type": "String"
										}
